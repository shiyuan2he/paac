<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN"  
"http://tuckey.org/res/dtds/urlrewrite2.6.dtd">
<urlrewrite>
	<!-- 首页转发 -->
	<rule>
		<from>^/index.jsp$</from>
		<to type="forward">/website/main.jsp</to>
	</rule>
	<outbound-rule>
		<from>^/website/main.jsp$</from>
		<to>/main.html</to>
	</outbound-rule>
</urlrewrite>

